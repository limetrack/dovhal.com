    "email-templates": "^4.0.1",
    "express": "^4.16.3",
    "express-sslify": "^1.2.0",
    "node-mailjet": "^3.2.1",
    "nodemailer": "^4.6.7"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/limetrack/dovhal.com.git"
  },
  "keywords": [
    "dovhal",
    "dovhal.com",
    "custom",
    "software",
    "development"
  ],
  "devDependencies": {}
